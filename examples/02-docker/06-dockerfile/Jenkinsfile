pipeline {
  // agent { dockerfile true }
  agent {
    dockerfile {
      dir 'examples/02-docker/dockerfile'
      filename 'Dockerfile'
      label 'docker'
    }
  }

  stages {
    stage('Test') {
      steps {
        sh '''
          node --version
          git --version
          curl --version
        '''
      }
    }
  }
}